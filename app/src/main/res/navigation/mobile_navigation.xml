<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_test">
    <fragment
        android:id="@+id/navigation_test"
        android:name="com.example.newsdemoapplication.ui.test.TestFragment"
        android:label="@string/title_test"
        tools:layout="@layout/fragment_test" >
        <action
            android:id="@+id/action_navigation_test_to_navigation_add_section"
            app:destination="@id/navigation_add_section" />
        <action
            android:id="@+id/action_navigation_test_to_photoViewPagerFragment"
            app:destination="@id/navigation_photoViewPagerFragment" />
    </fragment>
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.newsdemoapplication.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.newsdemoapplication.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.newsdemoapplication.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_add_section"
        android:name="com.example.newsdemoapplication.ui.add.AddSectionFragment"
        android:label="@string/title_Add"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_photoViewPagerFragment"
        android:name="com.example.newsdemoapplication.ui.viewpager.PhotoViewPagerFragment"
        android:label="PhotoViewPagerFragment" />


</navigation>